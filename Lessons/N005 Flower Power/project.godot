; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Turtle Lessons Template"
config/description="Шаблон с Черепашичем для изучения основ GDScript
Канал https://www.youtube.com/@GODOTru
Автор: Mikhail Bratus"
config/tags=PackedStringArray("lesson", "template")
run/main_scene="res://world.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://icon.svg"

[audio]

general/text_to_speech=true

[display]

window/size/viewport_width=640
window/size/viewport_height=640
window/size/resizable=false

[filesystem]

import/blender/enabled=false

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
